@model SistemaVeterinaria.Models.tbCita

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div>
    <h4>Cita</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Fecha Cita
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fecha_cita)
        </dd>

        <dt>
            Hora Cita
        </dt>

        <dd>
            @Html.DisplayFor(model => model.hora_cita)
        </dd>

        <dt>
            Descripcion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.descrip_cita)
        </dd>

        <dt>
            Fecha
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fecha)
        </dd>

        <dt>
            Nombre Empleado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbEmpleado.NOMBRE)
        </dd>

        <dt>
            Nombre Paciente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tbpaciente.nombre)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.cod_cita }, new { @class = "btn btn-warning mr-1" }) |
    @Html.ActionLink("Regresar", "Index", null, new { @class="btn btn-primary ml-1" })
</p>
