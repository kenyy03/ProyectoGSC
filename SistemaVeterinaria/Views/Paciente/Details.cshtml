@model SistemaVeterinaria.Models.Tbpaciente

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<div>
    <h4>Paciente</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre Paciente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            Fecha Nacimiento
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fecha_nac)
        </dd>

        <dt>
            Nombre Dueño
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbDUENO.nombre)
        </dd>

        <dt>
            Nombre Medico
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbEmpleado.NOMBRE)
        </dd>

        <dt>
            Nombre Especie
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbespecie.nombre)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.cod_paciente }, new { @class = "btn btn-warning mr-1" }) |
    @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-primary ml-1" })
</p>
