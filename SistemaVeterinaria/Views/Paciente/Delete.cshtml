@model SistemaVeterinaria.Models.Tbpaciente

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delete</h2>

<h3>¿ Estas seguro que quieres eliminar esto?</h3>
<div>
    <h4>Paciente</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre Paciente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            Fecha Nacimiento
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fecha_nac)
        </dd>

        <dt>
            Nombre Dueño
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbDUENO.nombre)
        </dd>

        <dt>
            Nombre Medico
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbEmpleado.NOMBRE)
        </dd>

        <dt>
            Nombre Especie
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tbespecie.nombre)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger mr-1" /> |
            @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-primary ml-1" })
        </div>
    }
</div>
